<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Csp:etc/csp_whitelist.xsd">
    <policies>
        <policy id="script-src">
            <values>
                <value id="cloudfront" type="host" algorithm="">*.cloudfront.net</value>
                <value id="hlx" type="host" algorithm="">*.hlx.page</value>
                <value id="localhost" type="host" algorithm="">localhost:3000</value>
            </values>
        </policy>
    </policies>
</csp_whitelist>
